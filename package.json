{
  "name": "rotiro",
  "version": "1.0.8",
  "description": "Rotiro API Middleware",
  "author": "Alex Everitt",
  "scripts": {
    "test": "jest",
    "test:ci": "jest --ci --coverage",
    "clear-build": "rm -rf lib/*",
    "build": "rm -rf lib/* && tsc",
    "prepublishOnly": "rm -rf lib/* && tsc && cp package.json lib/",
    "lint": "./node_modules/.bin/tslint -c tslint.json 'src/**/*.ts'",
    "lint-fix": "./node_modules/.bin/tslint -c tslint.json 'src/**/*.ts' --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rotirojs/rotiro.git"
  },
  "license": "MIT",
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@types/node": "^14.14.5",
    "jest": "^26.6.3",
    "prettier": "^1.19.1",
    "ts-jest": "^26.4.4",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "path-to-regexp": "^6.2.0"
  }
}
